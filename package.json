{
  "name": "chronicle",
  "version": "0.1.0",
  "scripts": {
    "dev": "nodemon",
    "build": "next build && tsc --project tsconfig.node.json",
    "start": "node build/index.js"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "mongodb": "^5.0.1",
    "next": "13.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "winston": "^3.8.2"
  },
  "nodemonConfig": {
    "exec": "ts-node --project tsconfig.node.json -r tsconfig-paths/register index.ts",
    "ext": "ts,tsx,css",
    "delay": 3000
  },
  "devDependencies": {
    "@next/font": "13.1.6",
    "@types/cookie": "^0.5.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/node": "18.13.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "tscpaths": "^0.0.9",
    "typescript": "^4.9.5"
  }
}
